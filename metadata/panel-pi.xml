<?xml version="1.0"?>
<wf-panel-pi>
	<plugin name="panel">
	<_short>Panel</_short>
	<category>Shell</category>
	<group>
	<_short>General</_short>
	<option name="css_path" type="string">
		<_short>CSS Path</_short>
		<default></default>
	</option>
	<option name="widgets_left" type="string">
		<_short>Widgets Left</_short>
		<default>smenu spacing4 launchers spacing8 window-list</default>
	</option>
	<option name="widgets_center" type="string">
		<_short>Widgets Center</_short>
		<default>none</default>
	</option>
	<option name="widgets_right" type="string">
		<_short>Widgets Right</_short>
		<default>tray power ejecter updater spacing2 bluetooth spacing2 netman spacing2 volumepulse spacing2 clock spacing2 batt spacing2</default>
	</option>
	<option name="widgets_all" type="string">
		<_short>All Available Widgets</_short>
		<default>batt bluetooth clock cpu cputemp ejecter gpu kbswitch launchers netman power smenu tray updater volumepulse window-list</default>
	</option>
	<option name="icon_size" type="int">
		<_short>Icon Size</_short>
		<default>32</default>
	</option>
	<option name="minimal_height" type="int">
		<_short>Minimal Height</_short>
		<default>24</default>
	</option>
	<option name="autohide" type="bool">
		<_short>Autohide</_short>
		<default>false</default>
	</option>
	<option name="autohide_duration" type="int">
		<_short>Autohide Duration</_short>
		<default>300</default>
	</option>
	<option name="monitor" type="int">
		<_short>Monitor for Panel</_short>
		<default>0</default>
	</option>
	<option name="position" type="string">
		<_short>Panel Position</_short>
		<default>top</default>
		<desc>
			<value>top</value>
			<_name>Top</_name>
		</desc>
		<desc>
			<value>bottom</value>
			<_name>Bottom</_name>
		</desc>
	</option>
	<option name="notify_enable" type="bool">
		<_short>Enable Notifications</_short>
		<default>true</default>
	</option>
	<option name="notify_timeout" type="int">
		<_short>Notification Timeout</_short>
		<default>15</default>
	</option>
	<option name="edge_offset" type="int">
		<_short>Edge offset</_short>
		<_long>The distance from the cursor to the edge of screen to show the panel when it's hidden.</_long>
		<default>20</default>
	</option>
	<option name="layer" type="string">
		<_short>Layer</_short>
		<default>bottom</default>
		<desc>
			<value>top</value>
			<_name>Top</_name>
		</desc>
		<desc>
			<value>bottom</value>
			<_name>Bottom</_name>
		</desc>
		<desc>
			<value>overlay</value>
			<_name>Overlay</_name>
		</desc>
		<desc>
			<value>background</value>
			<_name>Background</_name>
		</desc>
	</option>
	<option name="background_color" type="string">
		<_short>Background Color</_short>
		<default>#EDECEBFF</default>
	</option>
	</group>
	<group>
	<_short>Launchers</_short>
	<option name="launchers_spacing" type="int">
		<_short>Launchers Spacing</_short>
		<default>0</default>
	</option>
	<option name="launchers_size" type="int">
		<_short>Launchers Size</_short>
		<default>48</default>
	</option>
	<option name="launchers_animation_duration" type="int">
		<_short>Animation Duration</_short>
		<_long>High values may cause jittery animations</_long>
		<default>200</default>
	</option>
	</group>
	<group>
	<_short>Clock</_short>
	<option name="clock_format" type="string">
		<_short>Clock Format</_short>
		<default>%H:%M</default>
	</option>
	<option name="clock_font" type="string">
		<_short>Clock Font</_short>
		<default>default</default>
	</option>
	</group>
	<group>
	<_short>Window List</_short>
	<option name="window-list_max_width" type="int">
		<_short>Window List Maximum Task Width</_short>
		<default>200</default>
	</option>
	</group>
	<group>
	<_short>Searchable Menu</_short>
	<option name="smenu_search_height" type="int">
		<_short>Searchable Menu Search Window Height</_short>
		<default>300</default>
	</option>
	<option name="smenu_search_fixed" type="bool">
		<_short>Searchable Menu Fix Height of Search Window</_short>
		<default>false</default>
	</option>
	</group>
	<group>
	<_short>CPU</_short>
	<option name="cpu_show_percentage" type="bool">
		<_short>CPU Show Percentage</_short>
		<default>true</default>
	</option>
	<option name="cpu_foreground" type="string">
		<_short>CPU Foreground Colour</_short>
		<default>dark gray</default>
	</option>
	<option name="cpu_background" type="string">
		<_short>CPU Background Colour</_short>
		<default>light gray</default>
	</option>
	</group>
	<group>
	<_short>GPU</_short>
	<option name="gpu_show_percentage" type="bool">
		<_short>GPU Show Percentage</_short>
		<default>true</default>
	</option>
	<option name="gpu_foreground" type="string">
		<_short>GPU Foreground Colour</_short>
		<default>dark gray</default>
	</option>
	<option name="gpu_background" type="string">
		<_short>GPU Background Colour</_short>
		<default>light gray</default>
	</option>
	</group>
	<group>
	<_short>CPU Temperature</_short>
	<option name="cputemp_foreground" type="string">
		<_short>CPU Temperature Foreground Colour</_short>
		<default>dark gray</default>
	</option>
	<option name="cputemp_background" type="string">
		<_short>CPU Temperature Background Colour</_short>
		<default>light gray</default>
	</option>
	<option name="cputemp_throttle_1" type="string">
		<_short>CPU Temperature Throttle 1 Colour</_short>
		<default>orange</default>
	</option>
	<option name="cputemp_throttle_2" type="string">
		<_short>CPU Temperature Throttle 2 Colour</_short>
		<default>red</default>
	</option>
	<option name="cputemp_low_temp" type="int">
		<_short>CPU Temperature Low Temperature</_short>
		<default>40</default>
	</option>
	<option name="cputemp_high_temp" type="int">
		<_short>CPU Temperature High Temperature</_short>
		<default>90</default>
	</option>
	</group>
	<group>
	<_short>Updater</_short>
	<option name="updater_interval" type="int">
		<_short>Updater Interval Between Checks In Hours</_short>
		<default>24</default>
	</option>
	</group>
	<group>
	<_short>Ejecter</_short>
	<option name="ejecter_autohide" type="bool">
		<_short>Ejecter Hide When Nothing To Eject</_short>
		<default>true</default>
	</option>
	</group>
	<group>
	<_short>Battery</_short>
	<option name="batt_batt_num" type="int">
		<_short>Battery Battery Number</_short>
		<default>0</default>
	</option>
	</group>
	<group>
	<_short>Tray</_short>
	<option name="tray_smooth_scrolling_threshold" type="int">
		<_short>Smooth Scrolling Threshold</_short>
		<default>5</default>
		<min>1</min>
	</option>
	<option name="tray_menu_on_middle_click" type="bool">
		<_short>Middle Button Activates Menu</_short>
		<_long>Mouse right button activates a tray item's secondary action instead of the item's menu.</_long>
		<default>false</default>
	</option>
	</group>
	<group>
	<_short>Keyboard Switcher</_short>
	<option name="kbswitch_keyboard_0" type="string">
		<_short>Keyboard 0</_short>
		<default>none</default>
	</option>
	<option name="kbswitch_keyboard_1" type="string">
		<_short>Keyboard 1</_short>
		<default>none</default>
	</option>
	<option name="kbswitch_keyboard_2" type="string">
		<_short>Keyboard 2</_short>
		<default>none</default>
	</option>
	<option name="kbswitch_keyboard_3" type="string">
		<_short>Keyboard 3</_short>
		<default>none</default>
	</option>
	<option name="kbswitch_keyboard_4" type="string">
		<_short>Keyboard 4</_short>
		<default>none</default>
	</option>
	</group>
	</plugin>
</wf-panel-pi>
